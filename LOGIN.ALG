Algoritmo "login"


Var
   nome, senha: caracter
   
Inicio

   enquanto ((nome <> "Marcos") ou (senha <> "paylivre")) faca
      escreva("Digite o nome correspondente: ")
      leia(nome)
      escreva("Digite a senha correspondente: ")
      leia(senha)
   fimenquanto
   escreva("Seja Bem-Vindo ", nome)
Fimalgoritmo