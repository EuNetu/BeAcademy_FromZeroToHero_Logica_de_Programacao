Algoritmo "tabuada_while"

Var
   numeroSelecionado, j:inteiro


Inicio
   escreva("Digite o numero que voce deseja saber a tabuada: ")
   leia(numeroSelecionado)
   enquanto j <= 10 faca
      escreval(numeroSelecionado, " * ", j, " = ", (numeroSelecionado*j))
      j <- j + 1
   fimenquanto
Fimalgoritmo